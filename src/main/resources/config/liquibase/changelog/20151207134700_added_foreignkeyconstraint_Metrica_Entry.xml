<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <!--
        Added Foreign Key para entidades Metrica y Entry.
    -->
    <changeSet id="20151207134700" author="fer">

        <addForeignKeyConstraint baseColumnNames="entry_id"
                                 baseTableName="metrica"
                                 constraintName="fk_metrica_entry_id"
                                 referencedColumnNames="id"
                                 referencedTableName="entry"/>

        <addForeignKeyConstraint baseColumnNames="goal_id"
                                 baseTableName="entry"
                                 constraintName="fk_entry_goal_id"
                                 referencedColumnNames="id"
                                 referencedTableName="goal"/>

    </changeSet>
</databaseChangeLog>
